{
  "functions": {
    "api/index.js": {
      "memory": 128,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/check_access",
      "destination": "/api/geoblock.js"
    },
    {
      "source": "/tunnel",
      "destination": "/api/sentry.js"
    },
    {
      "source": "/marketdata/v1",
      "destination": "/api/marketdata.js"
    },
    {
      "source": "/(api|onramp-api|btc-api)/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
